        <typeAlias type="com.zhiyou100.model.Line" alias="Line"/>
        <typeAlias type="com.zhiyou100.model.Message" alias="Message"/>
        <typeAlias type="com.zhiyou100.model.Mold" alias="Mold"/>
        <typeAlias type="com.zhiyou100.model.Movie" alias="Movie"/>
        <typeAlias type="com.zhiyou100.model.Speaker" alias="Speaker"/>
        <typeAlias type="com.zhiyou100.model.Theme" alias="Theme"/>
        <typeAlias type="com.zhiyou100.model.ThemeVideo" alias="ThemeVideo"/>
        <typeAlias type="com.zhiyou100.model.User" alias="User"/>
