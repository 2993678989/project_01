        <typeAlias type="com.zhiyou100.model.Actor" alias="Actor"/>
        <typeAlias type="com.zhiyou100.model.Admin" alias="Admin"/>
        <typeAlias type="com.zhiyou100.model.Area" alias="Area"/>
        <typeAlias type="com.zhiyou100.model.Banner" alias="Banner"/>
        <typeAlias type="com.zhiyou100.model.Channel" alias="Channel"/>
